{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "outFile": "ts.js",
    "sourceMap": true,
    "target": "es5",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  },
  "files": [
    "./Backend/BTreeDictionary.ts",
    "./Features/Feature.ts",
    "./Features/FBrand.ts",
    "./Features/FImage.ts",
    "./Features/FLink.ts",
    "./Features/FPrice.ts",
    "./Features/FTitle.ts",
    "./Backend/GC_Grouper.ts",
    "./Backend/Classify.ts",
    "./Backend/MathUnit.ts",
    "./Backend/App.ts",
    "./Frontend/app.main.ts",
    "./Frontend/app.component.ts",
    "./Frontend/app.module.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
